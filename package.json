{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "apotek-klinik-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"yarn tailwind:watch\" \"vite\" --names \"tailwind,vite\" --prefix-colors \"blue,green\"",
    "dev:vite": "vite",
    "build": "yarn tailwind:build && tsc -b && vite build",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "preview": "vite preview",
    "tailwind:watch": "npx tailwindcss -i ./src/App.scss -o ./src/output.css --watch",
    "tailwind:build": "npx tailwindcss -i ./src/App.scss -o ./src/output.css",
    "add-admin": "tsx scripts/add-admin-user.ts",
    "add-admin:help": "tsx scripts/add-admin-user.ts --help",
    "update-password": "tsx scripts/update-user-password.ts",
    "update-password:help": "tsx scripts/update-user-password.ts --help",
    "export": "tsx scripts/export.ts",
    "loc-stats": "tsx scripts/loc-stats.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@supabase/supabase-js": "latest",
    "@tailwindcss/cli": "^4.1.12",
    "@tanstack/react-query": "^5.85.3",
    "@tanstack/react-query-devtools": "^5.85.3",
    "ag-grid-enterprise": "^34.1.1",
    "ag-grid-react": "^34.1.1",
    "axios": "^1.11.0",
    "chart.js": "^4.5.0",
    "classnames": "^2.5.1",
    "compressorjs": "^1.2.1",
    "framer-motion": "^12.23.12",
    "fuzzysort": "^3.1.0",
    "google-auth-library": "^10.2.1",
    "googleapis": "^156.0.0",
    "react": "^19.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.1",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.8.1",
    "react-spinners": "^0.17.0",
    "zod": "^4.0.17",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/classnames": "^2.3.4",
    "@types/node": "^24.3.0",
    "@types/pg": "^8.15.5",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react-swc": "^4.0.0",
    "concurrently": "^9.2.0",
    "dotenv": "^17.2.1",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "pg": "^8.16.3",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "sass": "^1.90.0",
    "supabase": "^2.37.0",
    "tailwindcss": "^4.1.12",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0",
    "vite": "^7.1.2"
  },
  "packageManager": "yarn@3.2.3+sha512.f26f951f67de0c6a33ee381e5ff364709c87e70eb5e65c694e4facde3512f1fa80b8679e6ba31ce7d340fbb46f08dd683af9457e240f25a204be7427940d767e"
