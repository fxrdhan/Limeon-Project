{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "apotek-klinik-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"yarn tailwind:watch\" \"vite\" --names \"tailwind,vite\" --prefix-colors \"blue,green\"",
    "dev:vite": "vite",
    "build": "yarn tailwind:build && tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "tailwind:watch": "npx tailwindcss -i ./src/App.css -o ./src/output.css --watch",
    "tailwind:build": "npx tailwindcss -i ./src/App.css -o ./src/output.css",
    "add-admin": "tsx scripts/add-admin-user.ts",
    "add-admin:help": "tsx scripts/add-admin-user.ts --help",
    "update-password": "tsx scripts/update-user-password.ts",
    "update-password:help": "tsx scripts/update-user-password.ts --help",
    "export": "tsx scripts/export.ts"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@supabase/supabase-js": "^2.50.0",
    "@tailwindcss/cli": "^4.1.10",
    "@tanstack/react-query": "^5.81.2",
    "@tanstack/react-query-devtools": "^5.81.2",
    "ag-grid-community": "^34.0.1",
    "ag-grid-react": "^34.0.1",
    "axios": "^1.10.0",
    "browser-image-compression": "^2.0.2",
    "chart.js": "^4.5.0",
    "dayjs": "^1.11.13",
    "framer-motion": "^12.18.1",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.6.2",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@tailwindcss/postcss": "^4.1.10",
    "@types/node": "^24.0.3",
    "@types/pg": "^8.11.6",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "concurrently": "^9.2.0",
    "dotenv": "^16.5.0",
