{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "pharmasys",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "license": "AGPL-3.0",
  "scripts": {
    "dev": "concurrently \"bun tailwind:watch\" \"bunx --bun vite\" --names \"tailwind,vite\" --prefix-colors \"blue,green\"",
    "dev:vite": "bunx --bun vite",
    "build": "bun tailwind:build && bunx --bun tsc -b && bunx --bun vite build",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "preview": "bunx --bun vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:coverage:open": "bun test:coverage && open coverage/index.html",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:changed": "vitest --changed",
    "test:related": "vitest --related",
    "tailwind:watch": "bunx --bun tailwindcss -i ./src/App.scss -o ./src/output.css --watch 2>&1 | grep -v 'watchman: not found'",
    "tailwind:build": "bunx --bun tailwindcss -i ./src/App.scss -o ./src/output.css",
    "add-admin": "bun scripts/add-admin-user.ts",
    "add-admin:help": "bun scripts/add-admin-user.ts --help",
    "update-password": "bun scripts/update-user-password.ts",
    "update-password:help": "bun scripts/update-user-password.ts --help",
    "export": "bun scripts/export.ts",
    "loc-stats": "bun scripts/loc-stats.ts",
    "gen:schemas": "bun gen:schemas:db && bun gen:schemas:forms && bun gen:schemas:invoice && bun gen:schemas:purchase",
    "gen:schemas:db": "bunx ts-to-zod --config=database --skipValidation",
    "gen:schemas:forms": "bunx ts-to-zod --config=forms --skipValidation",
    "gen:schemas:invoice": "bunx ts-to-zod --config=invoice --skipValidation",
    "gen:schemas:purchase": "bunx ts-to-zod --config=purchase --skipValidation",
    "prepare": "husky"
  },
  "dependencies": {
    "@ag-grid-community/locale": "35.0.0",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@supabase/supabase-js": "^2.89.0",
    "@tailwindcss/cli": "^4.1.18",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-query-devtools": "^5.91.1",
    "ag-grid-enterprise": "35.0.0",
    "ag-grid-react": "35.0.0",
    "axios": "^1.13.2",
    "browser-image-compression": "^2.0.2",
    "chart.js": "^4.5.1",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "compressorjs": "^1.2.1",
    "cropperjs": "1.6.2",
    "framer-motion": "^12.27.0",
    "fuzzysort": "^3.1.0",
    "google-auth-library": "^10.5.0",
    "googleapis": "^166.0.0",
    "motion": "^12.27.0",
    "react": "^19.2.3",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^19.2.3",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.12.0",
    "react-spinners": "^0.17.0",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.2.1",
    "zustand": "^5.0.9"
  },
  "overrides": {
    "tar": "^7.5.3",
    "js-yaml": "^4.1.1",
    "glob": "^11.1.0",
    "qs": "^6.14.1",
    "jws": "^4.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@eslint/js": "^9.39.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.4",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^16.5.0",
    "happy-dom": "^20.0.11",
    "husky": "^9.1.7",
    "jsdom": "^27.3.0",
    "lint-staged": "^16.2.7",
    "pg": "^8.16.3",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "rollup-plugin-visualizer": "^6.0.5",
    "sass": "^1.97.0",
    "supabase": "2.70.5",
    "tailwindcss": "^4.1.18",
    "ts-to-zod": "^5.1.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0",
    "typescript-language-server": "^5.1.3",
    "vite": "^7.3.0",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^4.0.16"
  }
}
