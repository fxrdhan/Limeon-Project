{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "apotek-klinik-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "license": "AGPL-3.0",
  "scripts": {
    "dev": "concurrently \"yarn tailwind:watch\" \"vite\" --names \"tailwind,vite\" --prefix-colors \"blue,green\"",
    "dev:vite": "vite",
    "build": "yarn tailwind:build && tsc -b && vite build",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:coverage:open": "yarn test:coverage && open coverage/index.html",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:changed": "vitest --changed",
    "test:related": "vitest --related",
    "tailwind:watch": "NODE_NO_WARNINGS=1 npx tailwindcss -i ./src/App.scss -o ./src/output.css --watch",
    "tailwind:build": "NODE_NO_WARNINGS=1 npx tailwindcss -i ./src/App.scss -o ./src/output.css",
    "add-admin": "tsx scripts/add-admin-user.ts",
    "add-admin:help": "tsx scripts/add-admin-user.ts --help",
    "update-password": "tsx scripts/update-user-password.ts",
    "update-password:help": "tsx scripts/update-user-password.ts --help",
    "export": "tsx scripts/export.ts",
    "loc-stats": "tsx scripts/loc-stats.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@supabase/supabase-js": "^2.81.1",
    "@tailwindcss/cli": "^4.1.17",
    "@tanstack/react-query": "^5.90.9",
    "@tanstack/react-query-devtools": "^5.90.2",
    "ag-grid-enterprise": "34.2.0",
    "ag-grid-react": "34.2.0",
    "axios": "^1.13.2",
    "chart.js": "^4.5.1",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "compressorjs": "^1.2.1",
    "framer-motion": "^12.23.24",
    "fuzzysort": "^3.1.0",
    "google-auth-library": "^10.5.0",
    "googleapis": "^166.0.0",
    "react": "^19.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^19.2.0",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.9.6",
    "react-spinners": "^0.17.0",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "resolutions": {
    "tar": "^7.5.2",
    "js-yaml": "^4.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.39.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "@vitest/coverage-v8": "^4.0.9",
    "@vitest/ui": "^4.0.9",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "happy-dom": "^20.0.10",
    "husky": "^9.1.7",
    "jsdom": "^27.2.0",
    "lint-staged": "^16.2.6",
    "pg": "^8.16.3",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "sass": "^1.94.0",
    "supabase": "2.58.5",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.2",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^4.0.9"
  },
  "packageManager": "yarn@4.10.3"
}
